module Model.BankBIC where

import qualified DA.List as L
import qualified DA.Set as S

template BankBIC
  with
    bic: Text
    banks: S.Set Party
    schedulers: S.Set Party
    assemblers: S.Set Party
  where
    signatory L.head $ S.toList banks
    observer schedulers, banks, assemblers
